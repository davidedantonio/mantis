<!doctype html>
<html>
	<head>
		<link rel="stylesheet" href="./assets/css/bootstrap.css">
    <title><%= title %></title>
		<!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
	</head>
	<body ng-app="MantisApplication">
		<!-- growl message div -->
		<div growl></div>

		<div id='page-container'>
			<div id="header" class="header navbar navbar-bee navbar-fixed-top" role="navigation">
        <!-- begin container-fluid -->
        <div class="container-fluid">
          <!-- begin mobile sidebar expand / collapse button -->
          <div class="navbar-header">
            <button class="navbar-toggle collapsed" data-toggle="collapse" data-target="#myNavbar" aria-expanded="false" aria-controls="navbar">
								<span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a href="/dashboards#/" class="navbar-brand">
                <span class="navbar-logo">
                    <img src="./assets/images/mantis_logo.png" />
                </span>
            </a>
          </div>
          <!-- end mobile sidebar expand / collapse button -->

					<nav id="myNavbar" class="collapse navbar-collapse">
						<!-- begin header navigation right -->
						<ul class="nav navbar-nav navbar-right">
							<li ng-show="dashboardId" ng-controller="WidgetsController">
								<a href="#" ng-click="openCreateWidget('sm')">
									<i class="fa fa-plus"></i> Add Widget
								</a>
							</li>
							<li ng-show="dashboardId" data-ng-controller="DropdownDashboards">
									<div class="dropdown" dropdown on-toggle="toggled(open)">
										<a href class="dropdown-toggle" dropdown-toggle>
											<i class="fa fa-dashboard"></i> Dashboards
										</a>
										<ul class="dropdown-menu">
											<li ng-repeat="choice in items">
												<a href="/dashboards#/details/{{ choice._id }}"><i class="{{ choice.icon }}"></i> {{ choice.name }}</a>
											</li>
										</ul>
									</div>
							</li>
							<li>
								<a href="#">
									help
								</a>
							</li>
							<li>
								<a href="/signout">
									logout
								</a>
							</li>
						</ul>
						<!-- end header navigation right -->
					</div>
      </div>
      <!-- end container-fluid -->
    </div>
    <!-- end #header -->
		<div ng-view></div>
  </div>
  </div>

	<script src="./assets/js/dependencies/underscore.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular.min.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular-route.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular-resource.js"></script>

	<!--<script src="./assets/js/dependencies/jquery.min.js"></script-->
	<script src="http://code.jquery.com/jquery-1.12.0.min.js"></script>
  <script src="./assets/js/libraries/bootstrap.min.js"></script>
	<script src="./assets/js/dependencies/angular-gridster.min.js"></script>
	<script src="./assets/js/dependencies/angular-cookies.js"></script>
	<script src="https://cdn.socket.io/socket.io-1.3.7.js"></script>
  <script src="./assets/js/dependencies/angular-socket-io.js"></script>
  <script src="./assets/js/dependencies/compareTo.module.js"></script>
  <script src="./assets/js/dependencies/highcharts-ng.min.js"></script>
  <script src="./assets/js/dependencies/jquery.easing.min.js"></script>
	<script src="./assets/js/dependencies/angular-messages.min.js"></script>
  <script src="./assets/js/libraries/highcharts/highcharts.js"></script>
  <script src="./assets/js/libraries/highcharts/highcharts-more.js"></script>
  <script src="./assets/js/libraries/highcharts/solid-gauge.js"></script>
  <script src="./assets/js/libraries/highcharts/dark-unica.js"></script>
  <script src="./assets/js/libraries/jquery-ui/ui/minified/jquery-ui.min.js"></script>
  <script src="./assets/js/libraries/respond/respond.min.js"></script>
  <script src="./assets/js/libraries/slimscroll/jquery.slimscroll.min.js"></script>
	<script src="./assets/js/libraries/growl/angular-growl.min.js"></script>

  <!-- Load Modules -->
  <script src="./application.js"></script>
	<script src="./users/controllers/UsersController.js"></script>
	<script src="./dashboard/controllers/DashboardController.js"></script>
	<script src="./dashboard/controllers/DropdownDashboards.js"></script>
	<script src="./dashboard/controllers/ModalCreateDashboardController.js"></script>
	<script src="./dashboard/controllers/ModalDeleteDashboardController.js"></script>
	<script src="./widgets/controllers/WidgetsController.js"></script>
	<script src="./widgets/controllers/ModalWidgetsController.js"></script>
	<script src="./widgets/controllers/ModalDeleteWidget.js"></script>
	</body>
</html>
